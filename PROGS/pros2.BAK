PROCEDURE ijinACC
 IJIN =  ;
      (IJINMODUL('ANGG','ADMIN') .OR. IJINMODUL('SIMP','ADMIN') .OR.  ;
IJINMODUL('PINJ','ADMIN') .OR. IJINMODUL('AKT','ADMIN') .OR. IJINMODUL('LAIN','ADMIN'))
 RETURN IJIN
ENDPROC
